<launch>

  <include file="$(find rtt_ros)/launch/deployer.launch">
    <arg name="NAME" value="IRP6_OT_OROCOS_ROBOT"/>
    <arg name="DEPLOYER_ARGS" value="-s $(find irp6_bringup)/config/irp6-ot-hardware.ops"/>
    <arg name="LOG_LEVEL" value="Warning"/>
    <arg name="DEBUG" value="false"/>
  </include>


  <include file="$(find irp6_bringup)/launch/irp6-ot-inside.launch" ns="IRP6_OT_OROCOS_ROBOT">
  </include>

  <group ns="IRP6_OT_OROCOS_ROBOT">
    <param name="Irp6otHardwareInterface_EC/master_config" command="cat $(find irp6_bringup)/config/master_config.yaml"/>
    <rosparam>
      Irp6otManager:
        hal_component_name: Irp6otHardwareInterface_EC
      Irp6otHardwareInterface:
        timeouts_to_print: 3
      ATI3084:
        test_mode: false
    </rosparam>
  </group>

</launch>
